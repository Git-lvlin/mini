
page {
  background-color: #f5f5f5;
}

.container {
  display: flex;
  flex-direction: column;
  height: 100vh;
  overflow: hidden;
  background-position: left top;
  background-size: 100% auto;
  background-repeat: no-repeat;
}

.home_content {
  position: relative;
  height: 100vh;
  padding-bottom: 144rpx;
  box-sizing: border-box;
}

.pul_icon {
  position: absolute;
  top: -130rpx;
  left: 50%;
  z-index: 0;
  width: 160rpx;
  height: 160rpx;
  transform: translateX(-50%);
}




.cart_popup {
  padding: 20rpx;
  
}
.box {
  background: #fff;
  border-radius: 14rpx;
}
.cart_popup_title {
  width: 100%;
  height: 114rpx;
  background: #F5F5F5;
  text-align: center;
  line-height: 114rpx;
  font-size: 36rpx;
  color: #333333;
}
.top {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  padding: 0 20rpx;
}
.top_left {
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.checkout_icon {
  width: 40rpx;
  height: 42rpx;
  flex-shrink: 0;
}
.top_left_text {
  margin-left: 18rpx;
  font-size: 28rpx;
  color: #000000;
}

.top_right {
  position: relative;
  margin-right: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  /* width: 160rpx; */
  height: 48rpx;
  line-height: 48rpx;
  background: #C7C7C7;
  border-radius: 24rpx;
  color: #FFFFFF;
  font-size: 24rpx;
}
.cart_null_box {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.cart_null_icon {
  width: 576rpx;
  height: 585rpx;
  margin-top: 48rpx;
}
.cart_null_text {
  position: relative;
  top: -38rpx;
  font-size: 26rpx;
  color: #333333;
}
.cart_null_btn {
  width: 200rpx;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  color: #FFFFFF;
  font-size: 32rpx;
  background-image: linear-gradient(95deg, #FF6E53 0%, #FF0000 100%);
  border-radius: 36rpx;

}

.btn_left {
  /* margin-right: 24rpx; */
  z-index: 3;
  width: 88rpx;
  text-align: center;
}
.btn_right {
  z-index: 3;
  width: 88rpx;
  text-align: center;
}
.btn_checkouted {
  position: absolute;
  top: 0;
  left: 88rpx;
  width: 88rpx;
  height: 100%;
  background: #FF2E23;
  border-radius: 12px;
  z-index: 2;
}
.self {
  position: absolute;
  top: 0;
  left: 0rpx;
  width: 88rpx;
  height: 100%;
  background: #FF2E23;
  border-radius: 12px;
  z-index: 2;
  margin-left: 0;
}
.list_box {
  width: 100%;
  overflow: hidden;
  overflow-y: auto;
  flex: 1;
  box-sizing: border-box;
}
.list_one {
  padding: 0 24rpx 22rpx 24rpx;
  display: flex;
  flex-direction: column;
}
.border {
  width: 100%;
  height: 24rpx;
  background: #F7F7F7;
}
.one_item {
  position: relative;
}
.one_item_left {
  display: flex;
  /* width: 563rpx; */
  height: 202rpx;
  justify-content: flex-start;
  align-items: center;
  overflow: hidden;
}
.goods_img {
  margin-left: 16rpx;
  margin-right: 20rpx;
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
  border-radius: 10rpx;
  flex-shrink: 0;
}
.goods_detail {
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  /* width: 308rpx; */
  height: 160rpx;
  overflow: hidden;
}
.goods_name {
  width: 100%;
  font-size: 28rpx;
  color: #222222;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.goods_price {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
  font-size: 20rpx;
}
.price_head {
  color: #FF1711;
}
.sale_price {
  position: relative;
  top: 4rpx;
  color: #FF1711;
  font-size: 32rpx;
}
.mark_price {
  margin-left: 10rpx;
  color: #B2B2B2;
  text-decoration: line-through;
}

.add-btn-box {
  position: absolute;
  right: 0;
  bottom: 26rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.step_delete_icon,.step_add_icon {
  width: 48rpx;
  height: 48rpx;
}
.step_index_num {
  margin: 0 8rpx;
  font-size: 36rpx;
  color: #333333;
}

.popup_in_cart {
  padding: 40rpx 0 48rpx 0;
  width: 590rpx;
  background: #FFFFFF;
  border-radius: 32rpx;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
}
.in_title {
  margin-bottom: 32rpx;
  font-size: 36rpx;
  font-weight: 600;
  color: #333333;
}
.in_content {
  margin-bottom: 48rpx;
  font-size: 32rpx;
  color: #333333;
  line-height: 42rpx;
}
.in_btn_box {
  display: flex;
  justify-content: center;
  align-items: center;
}
.cancel {
  margin-right: 30rpx;
  text-align: center;
  width: 216rpx;
  height: 72rpx;
  line-height: 72rpx;
  border-radius: 36rpx;
  border: 2rpx solid #CCCCCC;
  font-size: 28rpx;
  color: #333333;
}
.confirm {
  text-align: center;
  width: 216rpx;
  height: 72rpx;
  line-height: 72rpx;
  border-radius: 36rpx;
  background: #FF0000;
  font-size: 28rpx;
  color: #FFFFFF;
}

.list_two_top {
  padding: 24rpx 24rpx 0 24rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list_two_top_text {
  font-size: 28rpx;
  color: #000000;
}
.clear_btn {
  display: flex;
  justify-content: center;
  align-items: center;
}
.clear_icon {
  margin-right: 2rpx;
  width: 36rpx;
  height: 38rpx;
}
.clear_text {
  font-size: 24rpx;
  color: #E5352F;
}

.two_item {
  opacity: .5;
  position: relative;
  padding-left: 24rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  height: 202rpx;
}
.two_img_box {
  margin: 0 20rpx;
  position: relative;
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
}
.two_goods_img {
  width: 100%;
  height: 100%;
}
.invalid_icon {
  position: absolute;
  top: 0;
  left: 0;
  width: 160rpx;
  height: 160rpx;
}
.two_delete_btn{
  position: absolute;
  top: 84rpx;
  right: 24rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #666666;
}
.op {
  color: #666666;
}

.new-cart {
  right: 0;
  height: 144rpx;
  background: #fff;
  position: fixed;
  padding: 0 20px;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  bottom: 0;
  left: 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
  box-shadow: 0px -8px 8px 0px rgba(0, 0, 0, 0.05);
}

.new-cart_icon-wrap {
  text-align: center;
  margin-right: 40rpx;
  position: relative;
}

.new-cart_icon {
  width: 36rpx;
  height: 36rpx;
}

.new-cart_icon-text {
  font-size: 22rpx;
  color: #666;
}

.new-cart_icon-num {
  position: absolute;
  top: 0;
  right: 0;
  width: 34rpx;
  height: 34rpx;
  line-height: 34rpx;
  background: #E5352F;
  border-radius: 50%;
  transform: translate(25%,-50%);
  color: #fff;
  text-align: center;
  font-size: 20rpx;
}

.new-cart_btn-group {
  flex: 1;
  display: flex;
  overflow: hidden;
  justify-content: flex-end;
}


.new-cart_btn-right {
  width: 200rpx;
  line-height: 72rpx;
  text-align: center;
  font-size: 32rpx;
  border-radius: 36rpx;
  background: linear-gradient(92deg, #FF6E53 0%, #E5352F 99%);
  color: #fff;
}

.price-box {
  margin-right: 10rpx;
}

.price-box_price {
  font-size: 40rpx;
  color: #E5352F;
}

.price-box_text {
  font-size: 22rpx;
  color: #999999;
  text-align: right;
}

.solid {
  font-size: 20rpx;
  color: #999999;
}

.spec {
  background: #F2F2F2;
  border-radius: 20rpx;
  display: inline-block;
  font-size: 24rpx;
  padding: 5rpx 10rpx;
  color: #666;
  margin-top: 5rpx;
}

.spec_text {
  max-width: 250rpx;
  display: inline-block;
}

.spec_icon {
  width: 20rpx;
  height: 10rpx;
  position: relative;
  top: -8rpx;
}