<view class="bar_container">
  <view class="bar_item" wx:for="{{tabList}}" wx:key="index" data-path="{{item.pagePath}}" bindtap="onToPath">
    <image class="bar_icon" src="{{pagePath === item.pagePath ? item.selectedIconPath : item.iconPath}}" />
    <view class="bar_title {{pagePath === item.pagePath ? 'bar_title_act' : ''}}">{{item.name}}</view>
  </view>
</view>
