
page {
  background-color: #FFFFFF;
}

.container {
  display: flex;
  flex-direction: column;
  height: 100vh;
  overflow: hidden;
}

.nav_content {
  width: 100%;
  background-position: left bottom;
  background-size: 100% auto;
}

.nav_body {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 999;
  width: 100%;
  background-position: left bottom;
  background-size: 100% auto;
}

.nav_location {
  display: flex;
  align-items: center;
  height: 80rpx;
  padding-left: 24rpx;
}

.location_icon {
  width: 48rpx;
  height: 48rpx;
  margin-right: 4rpx;
}

.address {
  max-width: 420rpx;
  height: 48rpx;
  font-weight: 500;
  color: black;
  font-size: 32rpx;
  letter-spacing: 2rpx;
  line-height: 48rpx;
  margin-right: 8rpx;
}

.right_icon {
  width: 14rpx;
  height: 10rpx;
}

.home_content {
  top: 100rpx;
  position: relative;
  flex: 1;
  /* height: 1000rpx; */
}

.pul_icon {
  position: absolute;
  top: -120rpx;
  left: 50%;
  z-index: 0;
  width: 160rpx;
  height: 160rpx;
  transform: translateX(-50%);
}

.bar_height {
  height: 98rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
}

.search_content {
  height: 102rpx;
  box-sizing: border-box;
  padding: 16rpx 24rpx;
}

.search_body {
  display: flex;
  align-items: center;
  width: 100%;
  height: 68rpx;
  padding: 0 2rpx 0 16rpx;
  background-color: #fff;
  box-sizing: border-box;
  border: 4rpx solid #FF0000;
  border-radius: 56rpx;
}

.search_icon {
  width: 34rpx;
  height: 34rpx;
  margin-right: 12rpx;
}

.search_text {
  flex: 1;
  color: #999; 
  font-size: 28rpx;
  line-height: 40rpx;
}

.search_btn {
  width: 104rpx;
  height: 56rpx;
  color: #ffffff;
  font-size: 28rpx;
  letter-spacing: 2rpx;
  line-height: 56rpx;
  text-align: center;
  background: linear-gradient(90deg, #FF6E53 0%, #FF0000 100%);
  /* background: #FE4F8F; */
  border-radius: 56rpx
}
.block {
  width: 100%;
  height: 24rpx;
}
.null {
  margin-top: 120rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.null_img {
  width: 320rpx;
  height: 320rpx;
  margin-bottom: 16rpx;
}
.null_text {
  font-size: 32rpx;
  color: #888888;
}
.tips {
  margin: 0 auto;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 702rpx;
  height: 68rpx;
  background-color: #FFFFFF;
  box-shadow: 2rpx 0px 8rpx 0px rgba(0, 0, 0, 0.02);
  border-radius: 16rpx;
}
.tips_img {
  margin-left: 20rpx;
  margin-right: 24rpx;
  width: 24rpx;
  height: 26rpx;
}
.tips_text {
  font-size: 26rpx;
  color: #FF0000;
}

.cart_box {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  border-top: 2rpx solid #D8D8D8;
}
.cart_left_box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  background-color: #F7F7F7;
  overflow: hidden;
  overflow-y: auto;
}
.cart_right_box {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background-color: #ffffff;
  height: 1000rpx;
  overflow: hidden;
  overflow-y: auto;
}
.class_item_box {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 168rpx;
  height: 100rpx;
  color: #444440;
  font-size: 24rpx;
  background-color: #F7F7F7;
}
.left_act {
  color: #FF0000;
  background-color: #ffffff;
}
.left_act::after {
  content: '';
  position: absolute;
  top: 0;
  left: 0;
  width: 8rpx;
  height: 100%;
  background-color: #FF0000;
}
.goods_item_box {
  margin-left: 16rpx;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  border-bottom: 2rpx solid #D8D8D8;
  width: 566rpx;
  height: 200rpx;
}
.goods_img_box {
  margin-right: 24rpx;
  width: 160rpx;
  height: 160rpx;
  overflow: hidden;
}
.goods_img_index {
  width: 100%;
  height: 100%;
}
.goods_right_box {
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  justify-content: center;
  width: 382rpx;
  height: 100%;
}
.goods_name {
  width: 308rpx;
  height: 40rpx;
  font-size: 28rpx;
  color: #222222;
  line-height: 40rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
}
.goods_nums {
  margin-top: 4rpx;
  margin-bottom: 32rpx;
  width: 156rpx;
  height: 34rpx;
  font-size: 24rpx;
  color: #999999;
  line-height: 34rpx;
}
.step_box {
  position: absolute;
  right: 16rpx;
  bottom: 24rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.step_delete_icon,.step_add_icon {
  width: 48rpx;
  height: 48rpx;
}
.step_index_num {
  margin: 0 8rpx;
  font-size: 36rpx;
  color: #333333;
}

.goods_price_box {
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
  width: 100%;
}
.price_index {
  display: flex;
  align-items: flex-end;
  margin-right: 8rpx;
  color: #FF0000;
  font-size: 24rpx;
}
.a_price {
  font-size: 32rpx;
}
.price_old {
  font-size: 24rpx;
  color: #B2B2B2;
  text-decoration: line-through;
}

.cart_fixed {
  position: fixed;
  display: flex;
  justify-content: center;
  align-items: center;
  bottom: 98rpx;
  padding-bottom: constant(safe-area-inset-bottom);
  padding-bottom: env(safe-area-inset-bottom);
  width: 100%;
  height: 120rpx;
  background: #F7F7F7;
}

.cart_text {
  margin-left: 8rpx;
  margin-left: 18rpx;
  color: #333333;
  font-size: 30rpx;
}
.order_btn {
  margin-left: 158rpx;
  width: 200rpx;
  height: 72rpx;
  background: linear-gradient(145deg, #FF6E53 0%, #FF0000 100%);
  border-radius: 36rpx;
  text-align: center;
  line-height: 72rpx;
  font-size: 32rpx;
  color: #FFFFFF;
}

.cart_icon_box {
  position: relative;
}
.cart_icon {
  width: 72rpx;
  height: 76rpx;
}
.cart_num_box {
  box-sizing: border-box;
  position: absolute;
  top: 12rpx;
  right: 12rpx;
  width: 32rpx;
  height: 32rpx;
  border-radius: 50%;
  line-height: 32rpx;
  text-align: center;
  background: #FFFFFF;
  border: 2rpx solid #FF0000;
  line-height: 12rpx;
  color: #FF0000;
  font-size: 22rpx;
}
.cart_num_two {
  width: 40rpx;
  border-radius: 16rpx;
}
.cart_num_more {
  width: 40rpx;
  border-radius: 16rpx;
}

.cartPopup {
  background: #F5F5F5;
  width: 100%;
  height: 100%;
  overflow: hidden;
  display: flex;
  flex-direction: column;
}
.cart_title {
  text-align: center;
  width: 100%;
  height: 114rpx;
  line-height: 114rpx;
  font-size: 36rpx;
  color: #333333;
}
.cart_list_box {
  padding: 24rpx;
  background-color: #FFFFFF;
  overflow: hidden;
}
.cart_list {
  height: 420rpx;
  overflow: hidden;
  overflow-y: auto;
}
.cart_head {
  display: flex;
  justify-content: flex-start;
  align-items: center;
  position: relative;
  margin-bottom: 20rpx;
}
.cart_head_check {
  margin-right: 18rpx;
  width: 40rpx;
  height: 42rpx;
  overflow: hidden;
}
.cart_check_icon {
  width: 100%;
  height: 100%;
}
.head_all_text {
  font-size: 28rpx;
  color: #000000;
}
.head_right_box {
  position: absolute;
  right: 0;
  top: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 160rpx;
  height: 48rpx;
  line-height: 48rpx;
  background: #C7C7C7;
  border-radius: 24rpx;
  font-size: 12px;
  color: #FFFFFF;
}
.self {
  margin-left: 24rpx;
  z-index: 3;
}
.distribution {
  z-index: 3;
}
.red_bg {
  position: absolute;
  right: 0;
  width: 88rpx;
  height: 48rpx;
  background: #FF2E23;
  border-radius: 24rpx;
  z-index: 2;
}

.cart_goods_item {
  position: relative;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  height: 202rpx;
  overflow: hidden;
}
.cart_label {
  width: 40rpx;
  height: 42rpx;
  overflow: hidden;
}
.cart_goods_img_box {
  margin-left: 16rpx;
  width: 160rpx;
  height: 160rpx;
  background-color: green;
  overflow: hidden;
}
.cart_right_box_popup {
  margin-left: 20rpx;
  display: flex;
  flex-direction: column;
  width: 308rpx;
  height: 100%;
  justify-content: center;
  align-items: flex-start;
}
.cart_set_box {
  position: absolute;
  right: 0;
  bottom: 22rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.cart_goods_price_box {
  margin-top: 76rpx;
  display: flex;
  justify-content: flex-start;
  align-items: flex-end;
}
.s_price_head {
  font-size: 24rpx;
  color: #FF1711;
}
.s_price {
  font-size: 32rpx;
  color: #FF1711;
}
.m_price {
  margin-left: 8rpx;
  font-size: 10rpx;
  color: #B2B2B2;
  text-decoration: line-through;
}

.invalid_list_box {
  padding: 24rpx;
  display: flex;
  flex-direction: column;
  background-color: #FFFFFF;
}
.invalid_list {
  margin-top: 4rpx;
  display: flex;
  flex-direction: column;
  width: 100%;
  height: 202rpx;
  overflow: hidden;
  overflow-y: auto;
}
.invalid_goods_item {
  position: relative;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  width: 100%;
  height: 202rpx;
}
.invalid_delete {
  font-size: 28rpx;
  color: #666666;
}
.invalid_head_box {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.invalid_head_left {
  font-size: 28rpx;
  color: #000000;
}
.invalid_head_right {
  display: flex;
  justify-content: center;
  align-items: center;
  color: #999999;
  font-size: 24rpx;
}
.invalid_head_icon {
  width: 36rpx;
  height: 38rpx;
}

.border {
  width: 100%;
  height: 24rpx;
  background: #F7F7F7;
}
